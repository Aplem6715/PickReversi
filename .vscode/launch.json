            "name": "デバッグ",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetDirectory}/Reversi2023.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "X:/ProgramFiles/MinGW-w64/mingw64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "gdb の再フォーマットを有効にする",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Reversi2023"
        },
        {
            "name": "テスト実行",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetDirectory}/UnitTest.exe",
            "args": [
                "--gtest_color=yes"
